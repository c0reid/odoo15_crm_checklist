<odoo>
  <record id="view_crm_lead_form_inherit" model="ir.ui.view">
      <field name="name">crm.lead.inherit</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.crm_lead_view_form"/>
      <field name="arch" type="xml">
      <notebook position="inside">
        <page string="Checklist" name="checklist">
        <group string="Checklist">
        <field name="checkboxes_count" string="Checkboxes Count" read_only="True" force_save="True"/>
        <field name="checkbox_done_count" string="Checkboxes Done" read_only="True" force_save="True"/>  
        <field name="progress_rate" string="Progress" widget="progressbar" read_only="True" force_save="True"/>
        <field name="checkboxes_ids">  
          <tree editable="bottom">
           <!-- <tree multi_edit="1"> -->
          <field name="checkbox" widget='checkbox' />    
          <field name="name"/>
          <field name="description"/>
        </tree>
        </field>
        </group>
        </page>
        </notebook>
      </field>
    </record>

